                                <nav class="sb-sidenav-menu-nested nav">
                                    <a class="nav-link" href="~/Usuarios/Index">Criar Usuário</a>
                                    <a class="nav-link" href="~/Usuarios/Gerenciar">Gerenciar Usuários</a>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div class="small">Logado como:</div>
                    Usuário de Exemplo
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid">
                    <h1 class="mt-4">Gerenciar Usuários</h1>
                    <ol class="breadcrumb mb-4">
                        <li class="breadcrumb-item active"><p style="margin-bottom:0rem"><a href="~/Home/Index">Home</a> / Gerenciar Usuários</p></li>
                    </ol>
                    <br />
                    <div class="card mb-4">
                        <div class="card-header">
                            <i class="fas fa-users"></i>
                            Usuários Cadastrados
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered " id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Usuário</th>
                                            <th>Nome</th>
                                            <th>Função</th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                        @foreach(var usuario in Model){
                                            <tr>
                                                <td><a href="~/Usuarios/Editar/@usuario.Id">@usuario.Email</td>
                                                <td>@usuario.PrimeiroNome @usuario.SegundoNome</td>
                                                <td>@usuario.Funcao</td>

                                            </tr>
                                        }
                                        
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xl-3 col-md-6">

                        </div>
                        <div class="col-xl-3 col-md-6">

                        </div>
                        <div class="col-xl-3 col-md-6">

                        </div>
                        <div class="col-xl-3 col-md-6">

                        </div>
                    </div>


                </div>
            </main>
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright © Your Website 2020</div>
                        <div>
                            <a href="#">
                                Política de Privacidade
                            </a>
                            ·
                            <a href="#">
                                Termos e Condições
                            </a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
