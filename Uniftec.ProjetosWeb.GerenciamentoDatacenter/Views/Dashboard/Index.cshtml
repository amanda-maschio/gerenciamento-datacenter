                                                    from: 0,
                                                    to: 30,
                                                    color: '#55BF3B' // green
                                                }, {
                                                    from: 30,
                                                    to: 40,
                                                    color: '#DDDF0D' // yellow
                                                }, {
                                                    from: 40,
                                                    to: 100,
                                                    color: '#DF5353' // red
                                                }]
                                            },

                                            series: [{
                                                name: 'Temperatura',
                                                data: [80],
                                                tooltip: {
                                                    valueSuffix: ' ºC'
                                                }
                                            }]

                                        },
                                            // Add some life
                                            function (chart) {
                                                if (!chart.renderer.forExport) {

                                                    var model = @Html.Raw(Json.Encode(Model));
                                                    var temperatura = model.Sensor.Temperatura;

                                                    point = chart.series[0].points[0];
                                                    newVal = Math.trunc(temperatura);
                                                    point.update(newVal);

                                                }
                                            });
                                    </script>
