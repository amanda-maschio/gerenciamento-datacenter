                                                tickPixelInterval: 30,
                                                tickWidth: 2,
                                                tickPosition: 'inside',
                                                tickLength: 10,
                                                tickColor: '#666',
                                                labels: {
                                                    step: 2,
                                                    rotation: 'auto'
                                                },
                                                title: {
                                                    text: 'ºC'
                                                },
