<script>
    $(document).ready(function () {
        $("#tab2").hide();
        $("#tab3").hide();
        $("#tab1").show();
    });

    $('.nav-link').click(function () {
        $('.nav-link').removeClass('active');
        $(this).addClass('active');

        showTab($(this).attr("id"));
    });

    function showTab(nav) {
        if (nav == "nav1") {
            $("#tab2").hide();
            $("#tab3").hide();
            $("#tab1").show();
        } else if (nav == "nav2") {
            $("#tab1").hide();
            $("#tab3").hide();
            $("#tab2").show();
        } else if (nav == "nav3") {
            $("#tab1").hide();
            $("#tab2").hide();
            $("#tab3").show();
        }
    }
</script>
